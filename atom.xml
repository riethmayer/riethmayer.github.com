<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Tom Preston-Werner</title>
 <link href="http://riethmayer.de/atom.xml" rel="self"/>
 <link href="http://riethmayer.de/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://riethmayer.de/</id>
 <author>
   <name>Jan Riethmayer</name>
   <email>jan@riethmayer.de</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://riethmayer.de{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://riethmayer.de{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>